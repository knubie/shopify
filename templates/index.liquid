<div class="thirteen columns clearfix">
  <div class='carousel'>
    <img src="{{ 'carousel1.png' | asset_url }}" alt="" />
		<img src="{{ 'carousel2.png' | asset_url }}" alt="" />
  </div>

	{% if settings.home_featured %}
	  <div class='heading thirteen columns alpha'>featured products</div>
	  <div class='divider thirteen columns alpha'></div>
			{% for product in collections[settings.home_featured_collection].products %}
				{% include 'product_loop' %}
			{% endfor %}
	{% endif %}
	
	{% if settings.home_newest %}
	  <div class='heading thirteen columns alpha'>newest products</div>
	  <div class='divider thirteen columns alpha'></div>
			{% for product in collections[settings.home_newest_collection].products %}
				{% include 'product_loop' %}
			{% endfor %}
	{% endif %}
</div>